    <sys:String x:Key="ui_ConfigurationWindow_Submit_InstallOn">Level installed map</sys:String>
    <sys:String x:Key="ui_ConfigurationWindow_Submit_Map">Map</sys:String>
    <sys:String x:Key="ui_ConfigurationWindow_Submit_Score">Score (Counter value)</sys:String>
    <sys:String x:Key="ui_ConfigurationWindow_Submit_SRTime">SR Time (ms)</sys:String>
    <sys:String x:Key="ui_ConfigurationWindow_Submit_LifeUp">Life up</sys:String>
    <sys:String x:Key="ui_ConfigurationWindow_Submit_LifeLost">Life lost</sys:String>
    <sys:String x:Key="ui_ConfigurationWindow_Submit_ExtraPoint">Extra point collected</sys:String>
    <sys:String x:Key="ui_ConfigurationWindow_Submit_SubExtraPoint">Sub extra points collected</sys:String>
    <sys:String x:Key="ui_ConfigurationWindow_Submit_Trafo">Trafo passed</sys:String>
    <sys:String x:Key="ui_ConfigurationWindow_Submit_Checkpoint">Checkpoint passed</sys:String>
    <sys:String x:Key="ui_ConfigurationWindow_Submit_Date">Submit timestamp</sys:String>

    <sys:String x:Key="ui_ConfigurationWindow_Log_Title">Log</sys:String>
    
    

    <sys:String x:Key="ui_LoginUIControl_Username">User name:</sys:String>
    <sys:String x:Key="ui_LoginUIControl_Password">Password:</sys:String>
    <sys:String x:Key="ui_LoginUIControl_RememberName">Remember name</sys:String>
    <sys:String x:Key="ui_LoginUIControl_RememberPassword">Remember password</sys:String>
    <sys:String x:Key="ui_LoginUIControl_Login">Login</sys:String>

    
    
    <sys:String x:Key="ui_StatusBarUIControl_Network">Check ScoreManager-Magic services</sys:String>
    <sys:String x:Key="ui_StatusBarUIControl_Language">Check ScoreManager-Magic user interface language</sys:String>
    <sys:String x:Key="ui_StatusBarUIControl_Settings">Configure ScoreManager-Magic settings</sys:String>
    <sys:String x:Key="ui_StatusBarUIControl_Version">SMM version: v0.5</sys:String>

